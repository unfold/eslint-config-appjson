    "postpublish": "git push origin"
  },
  "eslintConfig": {
    "extends": "@unfold/eslint-config"
  },
  "babel": {
    "presets": [
      "@unfold/babel-preset"
    ]
  },
  "devDependencies": {
    "@unfold/eslint-config": "^1.3.2",
    "@unfold/babel-preset": "^1.0.2",
    "babel-cli": "^6.18.0",
    "eslint": "^3.12.2"
  },
  "dependencies": {
    "find-up": "^2.1.0"
  }
}
